<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.muliloda_multicalc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Muliloda_MultiCalc">

        <activity
            android:name=".Main.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Volume.VolumeChoose"
            android:exported="true" />
        <activity
            android:name=".Volume.Prism"
            android:exported="true" />
        <activity
            android:name=".Volume.Cube"
            android:exported="true" />
        <activity
            android:name=".Volume.Cone"
            android:exported="true" />
        <activity
            android:name=".Area.AreaChoose"
            android:exported="true" />
        <activity
            android:name=".Area.Square"
            android:exported="true" />
        <activity
            android:name=".Area.Rectangle"
            android:exported="true" />
        <activity
            android:name=".Area.Circle"
            android:exported="true" />
    </application>

</manifest>